<button {{action 'display'}}>Display</button>
<div class="table">
  <table style="width:100%">
    <thead>
    <tr>
      <th>Date</th>
      <th>Open</th>
      <th>High</th>
      <th>Low</th>
      <th>Close</th>
      <th>Volume</th>
    </tr>
  </thead>
    <tbody>
    {{#each model as |quote|}}
    <tr>
      <td>{{quote.date}}</td>
      <td>{{quote.open}}</td>
      <td>{{quote.high}}</td>
      <td>{{quote.low}}</td>
      <td>{{quote.close}}</td>
      <td>{{quote.volume}}</td>
    </tr>
    {{/each}}
  </tbody>
  </table>
</div>
<hr>
